<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tripschedule">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".ScheduleActivity"></activity>
        <activity android:name=".MapActivity" />
        <activity android:name=".WebView" />
        <activity android:name=".SelectBasket" />
        <activity
            android:name=".Splashscreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectLocation" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".TransportActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".BusTimeActivity" />
        <activity android:name=".TrainTimeActivity" />
        <activity android:name=".MemberAcivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".PassswordActivity" />
        <activity android:name=".SpeedBusTimeActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="7e7cc797q1" />



    </application>

</manifest>